{
    "name": "paysera/lib-rest-bundle",
    "description": "Helps easily convert your regular controller to RESTful controller",
    "autoload": {
        "psr-4": {
            "Paysera\\Bundle\\RestBundle\\": ""
        }
    },
    "require": {
        "php": "^7.2",
        "paysera/lib-serializer": "3.0.x-dev",
        "doctrine/orm": "^2.6",
        "symfony/http-kernel": "^4.2",
        "symfony/dependency-injection": "^4.2",
        "symfony/validator": "^4.2"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/victorlazov/lib-serializer"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^0.9.4"
    },
    "config": {
        "bin-dir": "bin"
    }
}
